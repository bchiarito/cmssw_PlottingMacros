#! /bin/bash

python plotting/plotter.py --var "TwoProng_pt[0]" --bins 100,0,2000 --logy --errors --file1 samples/trees_v2.0/signal/ --tree1 diphotonAnalyzer/fTree2 --file2 samples/trees_v2.0/SinglePhoton/ --file3 samples/trees_v2.0/GJets/ --dir --leg1 Signal_MC --leg2 SinglePhoton --leg3 GJets_MC --title "Leading TwoProng Object" --xaxis "p_{T}" --cut "nPass>=1 && nTightPhoton>=1" --name twoprong_pt --save --quiet
python plotting/plotter.py --var "TwoProng_phi[0]" --bins 64,-3.2,3.2 --logy --errors --file1 samples/trees_v2.0/signal/ --tree1 diphotonAnalyzer/fTree2 --file2 samples/trees_v2.0/SinglePhoton/ --file3 samples/trees_v2.0/GJets/ --dir --leg1 Signal_MC --leg2 SinglePhoton --leg3 GJets_MC --title "Leading TwoProng Object" --xaxis "\Phi" --cut "nPass>=1 && nTightPhoton>=1" --name twoprong_phi --save --quiet
python plotting/plotter.py --var "TwoProng_eta[0]" --bins 60,-3,3 --logy --errors --file1 samples/trees_v2.0/signal/ --tree1 diphotonAnalyzer/fTree2 --file2 samples/trees_v2.0/SinglePhoton/ --file3 samples/trees_v2.0/GJets/ --dir --leg1 Signal_MC --leg2 SinglePhoton --leg3 GJets_MC --title "Leading TwoProng Object" --xaxis "\eta" --cut "nPass>=1 && nTightPhoton>=1" --name twoprong_eta --save --quiet
python plotting/plotter.py --var "TwoProng_Mass[0]" --bins 100,0,100 --logy --errors --file1 samples/trees_v2.0/signal/ --tree1 diphotonAnalyzer/fTree2 --file2 samples/trees_v2.0/SinglePhoton/ --file3 samples/trees_v2.0/GJets/ --dir --leg1 Signal_MC --leg2 SinglePhoton --leg3 GJets_MC --title "Leading TwoProng Object" --xaxis "Mass" --cut "nPass>=1 && nTightPhoton>=1" --name twoprong_mass --save --quiet
python plotting/plotter.py --var "TwoProng_Mass[0]" --bins 40,0,10 --logy --errors --file1 samples/trees_v2.0/signal/ --tree1 diphotonAnalyzer/fTree2 --file2 samples/trees_v2.0/SinglePhoton/ --file3 samples/trees_v2.0/GJets/ --dir --leg1 Signal_MC --leg2 SinglePhoton --leg3 GJets_MC --title "Leading TwoProng Object" --xaxis "Mass" --cut "nPass>=1 && nTightPhoton>=1" --name twoprong_mass_zoom --save --quiet

python plotting/plotter.py --var "Photon1.pt" --bins 100,0,2000 --logy --errors --file1 samples/trees_v2.0/signal/ --tree1 diphotonAnalyzer/fTree2 --file2 samples/trees_v2.0/SinglePhoton/ --file3 samples/trees_v2.0/GJets/ --dir --leg1 Signal_MC --leg2 SinglePhoton --leg3 GJets_MC --title "Leading Photon (high pT ID)" --xaxis "p_{T}" --cut "nPass>=1 && nTightPhoton>=1" --name photon_pt --save --quiet
python plotting/plotter.py --var "Photon1.phi" --bins 64,-3.2,3.2 --logy --errors --file1 samples/trees_v2.0/signal/ --tree1 diphotonAnalyzer/fTree2 --file2 samples/trees_v2.0/SinglePhoton/ --file3 samples/trees_v2.0/GJets/ --dir --leg1 Signal_MC --leg2 SinglePhoton --leg3 GJets_MC --title "Leading Photon (high pT ID)" --xaxis "\Phi" --cut "nPass>=1 && nTightPhoton>=1" --name photon_phi --save --quiet
python plotting/plotter.py --var "Photon1.eta" --bins 60,-3,3 --logy --errors --file1 samples/trees_v2.0/signal/ --tree1 diphotonAnalyzer/fTree2 --file2 samples/trees_v2.0/SinglePhoton/ --file3 samples/trees_v2.0/GJets/ --dir --leg1 Signal_MC --leg2 SinglePhoton --leg3 GJets_MC --title "Leading Photon (high pT ID)" --xaxis "\eta" --cut "nPass>=1 && nTightPhoton>=1" --name photon_eta --save --quiet
