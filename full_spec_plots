#! /bin/sh
python single_dist_plotter.py --file samples/trees_v1.0/JetHT_runD/ --dir --var mEtaEta --min 0 --max 5000 --bins 71 --logy --error --cut "nPass==2 && dREtaEta>0.3 && Eta1.grommedMass < 2 && Eta2.grommedMass < 2" --name M_EtaEta_JetHT_massWindow_0to2000MeV --save
python single_dist_plotter.py --file samples/trees_v1.0/JetHT_runD/ --dir --var mEtaEta --min 0 --max 5000 --bins 71 --logy --error --cut "nPass==2 && dREtaEta>0.3 && Eta1.grommedMass < 2 && Eta2.grommedMass < 2 && abs(Eta1.eta - Eta2.eta) < 2" --name M_EtaEta_JetHT_massWindow_0to2000MeV_dEta_small --save
