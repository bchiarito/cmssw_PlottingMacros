#! /bin/bash
python plotting/plotter.py --var GammaTwoProng.mass --bins 60,0,3000 --logy --errors --file1 samples/trees_v2.0/signal/ --tree1 diphotonAnalyzer/fTree2  --dir --file2 samples/trees_v2.0/SinglePhoton/ --file3 samples/trees_v2.0/GJets/ --leg1 Signal_MC --leg2 SinglePhoton --leg3 GJets_MC --xaxis "\text{M}_{\gamma \text{TwoProng}}" --title "\text{Mass in 0-2000 MeV, cut on HT, }\Delta\eta \text{, }p_T(\gamma) \text{, Barrel}" --cut "nPass>=1 && nTightPhoton>=1 && GammaTwoProng.dEta<2 && GammaTwoProng.dR>0.3 && Photon1.pt>200 && TwoProng_Mass[0]>0 && TwoProng_Mass[0]<2.0 && Photon1.scEta < 1.4442" --name gammatwoprong_scan_0_2000_barrel --save

python plotting/plotter.py --var GammaTwoProng.mass --bins 60,0,3000 --logy --errors --file1 samples/trees_v2.0/signal/ --tree1 diphotonAnalyzer/fTree2  --dir --file2 samples/trees_v2.0/SinglePhoton/ --file3 samples/trees_v2.0/GJets/ --leg1 Signal_MC --leg2 SinglePhoton --leg3 GJets_MC --xaxis "\text{M}_{\gamma \text{TwoProng}}" --title "\text{Mass in 300-400 MeV, cut on HT, }\Delta\eta \text{, }p_T(\gamma) \text{, Barrel}" --cut "nPass>=1 && nTightPhoton>=1 && GammaTwoProng.dEta<2 && GammaTwoProng.dR>0.3 && Photon1.pt>200 && TwoProng_Mass[0]>0.3 && TwoProng_Mass[0]<0.4 && Photon1.scEta < 1.4442" --name gammatwoprong_scan_300_500_barrel --save
